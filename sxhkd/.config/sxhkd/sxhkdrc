super + b
  $BROWSER

super + F9
  mnt

super + F10
  umt

super + BackSpace
  power

super + e
  $GUIEDITOR

super + f
  $FILE

super + Return
  $TERMINAL

super + l
  lipsum

XF86AudioNext
  music playnext

XF86AudioPrev
  music playprev
  cmus-remote -r

XF86AudioPlay
  music playpause

XF86AudioRaiseVolume
  vol up

XF86AudioLowerVolume
  vol down

XF86AudioMute
  vol toggle
